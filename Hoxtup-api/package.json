{
  "name": "hoxtup-api",
  "version": "1.0.0",
  "description": "Hoxtup operational management API",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "dev:docker": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:db:push": "prisma db push --url postgresql://hoxtup:hoxtup_dev@localhost:5433/hoxtup_test",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@polar-sh/sdk": "^0.42.5",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "better-auth": "^1.4.18",
    "bullmq": "^5.67.3",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-openapi-validator": "^5.6.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "node-ical": "^0.25.1",
    "nodemailer": "^8.0.1",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "rate-limit-redis": "^4.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.18",
    "@eslint/js": "^10.0.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "@types/nodemailer": "^7.0.9",
    "@types/supertest": "^6.0.3",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.3.0",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
